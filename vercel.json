{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "https://server-tutifruti.vercel.app/socket.io/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://react-tutifruti.vercel.app"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}